<div class="record-of-handover">
  <div class="section-heading">
    <div class="heading">
      <h2><%= t('.heading') %></h2>
    </div>
    <div class="persistent-info">
      <span class="label"><%= t('pdfs.shared.identifiers_label') %></span>
      <div class="input"></div>
    </div>
  </div>
  <div class="property-details">
    <div class="heading"><%= t('.property_details') %></div>
    <div class="property-checkboxes">
      <div>
        <div class="check-box"></div>
        <div class="label-inline"><%= t('.property_outward_journey') %></div>
      </div>
      <div>
        <div class="check-box"></div>
        <div class="label-inline"><%= t('.property_return_journey') %></div>
      </div>
    </div>
    <ul>
      <li><strong><%= t('.cash_code') %></strong> - <%= t('.cash') %></li>
      <li><strong><%= t('.documentation_code') %></strong> - <%= t('.documentation') %></li>
      <li><strong><%= t('.in_possession_code') %></strong> - <%= t('.in_possession') %></li>
      <li><strong><%= t('.medication_code') %></strong> - <%= t('.medication') %></li>
      <li><strong><%= t('.open_at_court_code') %></strong> - <%= t('.open_at_court') %></li>
      <li><strong><%= t('.stored_property_code') %></strong> - <%= t('.stored_property') %></li>
      <li><strong><%= t('.valuables_code') %></strong> - <%= t('.valuables') %></li>
    </ul>
  </div>
  <div class="seal-numbers">
    <div>
      <table>
        <tr>
          <th><%= t('.code') %></th>
          <th><%= t('.seal_number') %></th>
          <th>&#163; <%= t('.value') %></th>
          <th><%= t('.initial') %></th>
        </tr>
        <% 10.times do %>
            <tr>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
            </tr>
        <% end %>
      </table>
    </div>
    <div>
      <table>
        <tr>
          <th><%= t('.reseal_no') %></th>
          <th><%= t('.reason_for_opening') %></th>
          <th><%= render('pdfs/clock_image') %></th>
          <th><%= t('.initial') %></th>
        </tr>
        <% 10.times do %>
            <tr>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
            </tr>
        <% end %>
      </table>
    </div>
  </div>
  <div class="handover">
    <div class="heading"><%= t('.handover') %></div>
    <p><%= t('.sign_to_confirm') %></p>
    <table class="no-border-left no-border-top">
      <tr>
        <td colspan="3" class="no-border"></td>
        <td colspan="2" class="no-border-top">
          <strong><%= t('.property_handover') %> <br> <%= t('.cash_and_medication') %></strong></td>
        <td colspan="2" class="no-border-top" style="width:30%"><strong><%= t('.detainee_handover') %></strong></td>
      </tr>
      <tr>
        <th class="no-border"></th>
        <th><%= render('pdfs/clock_image') %></th>
        <th></th>
        <th><strong><%= t('.name') %></strong></th>
        <th class="narrow"><strong><%= t('.initial') %></strong></th>
        <th><strong><%= t('.name') %></strong></th>
        <th class="narrow"><strong><%= t('.initial') %></strong></th>
      </tr>
      <tr>
        <td rowspan="2" class="no-border-left strong-border-bottom strong-border-right">
          <%= move.origin %>
        </td>
        <td></td>
        <td><%= t('.dispatching_officer') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr class="strong-border-bottom">
        <td></td>
        <td><%= t('.escort') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr>
        <td rowspan="2" class="no-border-left no-border-top strong-border-right">
          <%= move.destination %>
        </td>
        <td></td>
        <td><%= t('.escort') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr>
        <td></td>
        <td><%= t('.receiving_officer') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr>
        <td rowspan="2" class="no-border-left no-border-top strong-border-right strong-border-bottom"></td>
        <td></td>
        <td><%= t('.dispatching_officer') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr class="strong-border-bottom">
        <td></td>
        <td><%= t('.escort') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr>
        <td rowspan="2" class="no-border-left strong-border-right"></td>
        <td></td>
        <td><%= t('.escort') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr>
        <td></td>
        <td><%= t('.receiving_officer') %></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
    </table>
  </div>
</div>
